@using TravelTripProje.Models.Classes
@model List<Hakkimizda>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}


<div class="content">
	<div class="wrap">
		<div class="about">
			<h3>Hakkımızda</h3>
			@foreach (var item in Model)
			{
				<img src="@item.FotoUrl" alt="" />
				<p>@item.Explanation</p>
			}
			
		</div>
	</div>
</div>


